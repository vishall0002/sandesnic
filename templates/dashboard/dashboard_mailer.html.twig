{% extends 'bases/base_dashboard.html.twig' %}
{% block content %}
    <span id="__loginExpiresIn" data-stp="{{ path('app_ping') }}" data-sto="{{ sessionTimeOut }}" data-lp="{{ path('app_home') }}"></span>
    <div class="row">
        <div class="col">
            <h3 class="display-5">Chat Statistics</h3>
        </div>
        <div class="col text-right">
            <img alt="Digital India Logo" src="{{ asset('img/logo_di.png')}}" />
            <a class="btn btn-xs btn-primary"> <img style="height: 30px;" alt="National Informatics Centre Logo" src="{{ asset('img/logo_nic.png')}}" /></a>
            <div class="progress">
            <br />
            <button class="btn btn-xs btn-primary float-right fa fa-refresh" id="btnRefresh" data-api-path="{{path('app_dashboard_data_chats')}}" data-gd-dwpath="{{path('app_dashboard_gdata_dwchats')}}"  data-gd-path="{{path('app_dashboard_gdata_chats')}} " data-gd-lupath="{{path('app_dashboard_gdata_online')}} " data-gd-dwlupath="{{path('app_dashboard_gdata_dwonline')}} "></button>
                <div class="progress-bar progress-bar-striped bg-info" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <h6 class="last-updated-at text-right d-none d-md-block" style="width: 100%"></h6>
        </div>
    </div>
    {% include '/dashboard/dashboard_chats_mailer.html.twig' %}
{% endblock %}
{% block js %}
    <script src="{{ asset('build/js/dashboard.js') }}"></script>
{% endblock %}